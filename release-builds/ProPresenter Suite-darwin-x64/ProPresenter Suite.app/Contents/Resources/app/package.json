{
  "name": "ProPresenter-Suite",
  "productName": "ProPresenter Suite",
  "version": "0.1.1",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/pp-suite-icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . ProPresenter-Suite --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/pp-suite-icon.ico --prune=true --out=release-builds --version-string.CompanyName=JS --version-string.FileDescription=JS --version-string.ProductName=\"ProPresenter Suite\""
  },
  "dependencies": {
    "textract": "2.2.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "electron": "4.1.0",
    "electron-packager": "^13.1.1"
  }
}
